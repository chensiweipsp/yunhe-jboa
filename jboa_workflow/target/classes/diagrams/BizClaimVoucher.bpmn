        <activiti:taskListener event="create" class="utils.ManagerTaskHandler"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow5" sourceRef="usertask3" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${rollback=='no'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="总经理审批" activiti:assignee="${inputUser}" activiti:formKey="workflowAction_audit.action">
