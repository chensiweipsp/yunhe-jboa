        <activiti:taskListener event="create" class="utils.ManagerTaskHandler"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask4" name="财务审批" activiti:assignee="${inputUser}" activiti:formKey="workflowAction_audit.action">
